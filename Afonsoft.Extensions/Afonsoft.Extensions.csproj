<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net47;netcoreapp2.2</TargetFrameworks>
    <Authors>Afonso Dutra Nogueira Filho</Authors>
    <Company>Afonsoft</Company>
    <Copyright>afonsoft</Copyright>
    <PackageProjectUrl>https://github.com/afonsoft/Afonsoft.Extensions</PackageProjectUrl>
    <PackageTags>Afonsoft Extensions linq Enum Regex String Assembly DateTime Object Exception</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <AssemblyVersion>2019.7.12</AssemblyVersion>
    <FileVersion>2019.7.12</FileVersion>
    <SignAssembly>false</SignAssembly>
    <Version>2019.7.12</Version>
    <RepositoryUrl>https://github.com/afonsoft/Afonsoft.Extensions</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <Description>Afonsoft Extensions for linq, Enum, Regex, String, Assembly, DateTime, Object, Exception</Description>
    <NeutralLanguage>pt-BR</NeutralLanguage>
    <PackageLicenseUrl></PackageLicenseUrl>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <PackageReleaseNotes>Afonsoft Extensions for linq, Enum, Regex, String, Assembly, DateTime, Object, Exception</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="10.0.3" />
  </ItemGroup>

</Project>
